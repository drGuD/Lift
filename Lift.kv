                id: Txt_navodila
                text: root.navodila_text
                size_hint_y: None
                height: self.texture_size[1] if self.texture_size[1] > 0 else 1
                padding_y: 50
                color: 0, 0, 0, 1
                font_name: 'Roboto-Bold'

            Label:
                text: "Vpisovanje napredka"
                font_size:50
                bold:True
                size_hint_y: None
                color: 0, 0, 0, 1
                font_name: 'Roboto-Bold'

            GridLayout:
                cols: 2
                size_hint_y: None
                height: self.minimum_height
                spacing: 10

                Button:
                    id: Button_home
                    text: 'Home'
                    size_hint_y: None
                    height: dp(50)
                    background_color: 0.020, 0.396, 0.329, 1
                    font_name: 'Roboto-Bold'
                    background_normal: ''
                    background_down: ''
