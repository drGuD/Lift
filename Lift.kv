<Home_Widget>:

    canvas.before:
        Color:
            rgba: 1, 0.682, 0.012, 1
        Rectangle:
            pos: self.pos
            size: self.size

    GridLayout:
        cols: 1
        size_hint_y: None
        size: root.width, root.height
        spacing: 10

        Label:
            text: "Aplikacija LIFT"
            font_size:80
            color: 0, 0, 0, 1
            font_name: 'Roboto-Bold'

        Image:
            source: 'homepage.png'
            size_hint: (1.9, 1.9)  # Adjust the size as needed
            pos_hint: {'center_x': 0.5, 'center_y': 0.5}
        Label:
            text: "Treniraj zdaj:"
            color: 0, 0, 0, 1
            font_name: 'Roboto-Bold'

        Button:
            id: Button_roke
            text: "Treniraj roke!"
            font_name: 'Roboto-Bold'
            on_release: root.on_btn_roke_press()  # Call method on click
        Button:
            id: Button_noge
            font_name: 'Roboto-Bold'
            text: "Treniraj noge!"
            on_release: root.on_btn_noge_press()
            padding_y: 40

        Button:
            id: Button_statistika
            font_name: 'Roboto-Bold'
            text: "Statistika (Work in progress...)"

</Home_Widget>

<Roke_Widget>:
